<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.main.tracker">
    <!-- icon source: https://www.svgrepo.com/svg/108667/blood-drop -->
    <application android:name=".MensApplication"
        android:allowBackup="true"
        android:icon="@mipmap/menstracker_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/menstracker_launcher"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="com.main.tracker.activities.NewCycleActivity" />
        <activity android:name="com.main.tracker.activities.NextExpectedCycleActivity" />
        <activity android:name="com.main.tracker.activities.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>